<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/home_back"
    tools:context="xyz.codewithcoffee.cysapp.Timetable">

    <TextView
        android:id="@+id/rate_us_banner"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:gravity="center"
        android:text="Rate Us"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/cys"
        android:layout_width="130dp"
        android:layout_height="75dp"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/rate_us_banner"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cys_line" />

    <ImageButton
        android:id="@+id/submit_butt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:scaleX="0.75"
        android:scaleY="0.75"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ratingBar"
        app:srcCompat="@drawable/login_rect" />

    <TextView
        android:id="@+id/submit"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="Submit"
        android:textColor="#FFFFFF"
        android:textSize="27dp"
        app:layout_constraintBottom_toBottomOf="@+id/submit_butt"
        app:layout_constraintEnd_toEndOf="@+id/submit_butt"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/submit_butt"
        app:layout_constraintTop_toTopOf="@+id/submit_butt" />

    <ImageView
        android:id="@+id/sns"
        android:layout_width="247dp"
        android:layout_height="33dp"
        android:scaleX="13"
        android:scaleY="13"
        android:translationX="-17dp"
        android:translationY="-17dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/submit_butt"
        app:srcCompat="@drawable/contact_sns" />

    <ImageView
        android:id="@+id/rate_us"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-140dp"
        android:scaleX="1.5"
        android:scaleY="1.5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rate_us" />

    <RatingBar
        android:id="@+id/ratingBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="475dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rate_us_banner" />

</androidx.constraintlayout.widget.ConstraintLayout>